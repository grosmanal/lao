{{ form_start(careRequestForm, {
    'attr': {'onsubmit': careRequest.isActive ? 'submitCareRequest(event)' : 'reactivateCareRequest(event)'}
}) }}
<input type="hidden" name="care-request-id" value="{{ careRequest.id }}" />
